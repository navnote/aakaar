{
	"$schema": "https://json.schemastore.org/package",
	"name": "@aakaar/react",
	"version": "0.0.0",
	"exports": {
		".": "./src/index.ts"
	},
	"scripts": {
		"lint": "biome check .",
		"generate:component": "turbo gen react-component",
		"ts": "tsc --noEmit",
		"build": "tsc",
		"dev": "tsc --watch",
		"clean": "rm -rf dist",
